{
    "name": "phine/phar",
    "description": "A PHP library for creating and reading (without the phar extension) PHP archives.",
    "keywords": ["phar"],
    "homepage": "https://github.com/phine/lib-phar",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/phine/lib-phar/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "phine/exception": "~1.0",
        "phine/observer": "~2.0.0",
        "phine/path": "~1.0"
    },
    "require-dev": {
        "league/phpunit-coverage-listener": "~1.0",
        "phine/test": "~1.0"
    },
    "suggest": {
        "ext-phar": "For creating PHP archives."
    },
    "autoload": {
        "psr-0": {
            "Phine\\Phar": "src/lib"
        }
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
